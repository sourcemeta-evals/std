{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "x-license",
    "title",
    "description",
    "examples",
    "$comment",
    "$schema"
  ],
  "properties": {
    "$schema": {
      "const": "https://json-schema.org/draft/2020-12/schema"
    },
    "$id": false,
    "$comment": {
      "$ref": "./schemas/ietf/http/https-url.json"
    },
    "examples": {
      "type": "array",
      "minItems": 3
    },
    "x-license": {
      "const": "https://github.com/sourcemeta/std/blob/main/LICENSE"
    }
  }
}
